void sort(char s[][15])
{
    int i,j;
    char c[15];

    for(i=0;i<n-1;i++)
    {
      for(j=i+1;j<n;j++)
      {
       if(strcmp(s[i],s[j])>0)
       {
	strcpy(c,s[i]);
	strcpy(s[i],s[j]);
	strcpy(s[j],c);
       }
      }
    }
}

